{
    "name": "gigcodes/clickup-php",
    "description": "Fork ClickUp API client",
    "keywords": [
        "ClickUp",
        "click-up",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hayashi Takuya",
            "email": "qrbys@outlook.com"
        },
        {
            "name": "Eduard Samoilenko",
            "email": "eduard9969@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "caseyamcl/guzzle_retry_middleware": "^2.3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.40",
        "psy/psysh": "^0.11.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpstan/phpstan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ClickUp\\": "src/ClickUp/"
        }
    },
    "scripts": {
        "psysh": "@php ./vendor/bin/psysh"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}